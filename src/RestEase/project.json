{
  "version": "0.0.0",
  "title": "RestEase",
  "description": "Easy-to-use typesafe REST API client library, which is simple and customisable. Inspired by Refit",
  "authors": [ "Antony Male" ],
  "copyright": "Copyright 2015-2016 Antony Male",

  "dependencies": {
  },
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "NETSTANDARD"
        ]
      },
      "dependencies": {
        "System.Net.Http": "4.1.0",
        "System.Collections.Concurrent": "4.0.12",
        "System.Reflection.Emit": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "Newtonsoft.Json": "9.0.1",
        "Microsoft.AspNetCore.WebUtilities": "1.0.0"
      }
    },
    "net45": {
      "dependencies": {
        "Newtonsoft.Json": "6.0.6"
      },
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0",
        "System.Net.Http": "4.0.0.0"
      }
    },
    "net40": {
      "dependencies": {
        "Newtonsoft.Json": "6.0.6",
        "Microsoft.Bcl.Async": "1.0.168",
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0"
      }
    }
  },
  "buildOptions": {
    "emitEntryPoint": false,
    "xmlDoc": true
  },
  "packOptions": {
    "owners": [ "Antony Male" ],
    "licenseUrl": "https://github.com/canton7/RestEase/blob/master/LICENSE.txt",
    "projectUrl": "https://github.com/canton7/RestEase",
    "repository": {
      "type": "git",
      "url": "https://github.com/canton7/RestEase.git"
    },
    "tags": [ "REST", "JSON" ],
    "iconUrl": "https://raw.githubusercontent.com/canton7/RestEase/master/icon.png",
    "requireLicenseAcceptance": false
  }
}